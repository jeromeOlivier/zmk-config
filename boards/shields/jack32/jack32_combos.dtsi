/ {
    combos {
        compatible = "zmk,combos";
        combo_tilde {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <0 1>;
            bindings = <&kp TILDE>;
        };
        combo_minus {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <1 2>;
            bindings = <&kp MINUS>;
        };
        combo_plus {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <2 3>;
            bindings = <&kp PLUS>;
        };
        combo_grave {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <5 6>;
            bindings = <&kp GRAVE>;
        };
        combo_underscore {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <6 7>;
            bindings = <&kp UNDER>;
        };
        combo_underscore {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <7 8>;
            bindings = <&kp EQUAL>;
        };
        combo_underscore {
            timeout-ms = <50>;
            layers = <0>
            key-positions = <7 8>;
            bindings = <&kp EQUAL>;
        };
    };
};